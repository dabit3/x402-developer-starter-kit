{
  "name": "x402-developer-starter-kit",
  "version": "1.0.0",
  "description": "Starter kit for building paid APIs using the x402 payment protocol",
  "main": "dist/server.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "start": "node dist/server.js",
    "dev": "tsc && node dist/server.js",
    "test": "npm run build && node dist/testClient.js",
    "test:shell": "bash test-request.sh",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "x402",
    "payment-api",
    "payments",
    "usdc",
    "eip-3009"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@crossmint/wallets-sdk": "^0.14.0",
    "dotenv": "^16.4.5",
    "ethers": "^6.15.0",
    "express": "^4.21.2",
    "openai": "^4.78.0",
    "viem": "^2.33.1",
    "x402": "^0.7.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^24.6.2",
    "typescript": "^5.9.3"
  }
}
